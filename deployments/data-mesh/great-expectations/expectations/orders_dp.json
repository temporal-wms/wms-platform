{
  "expectation_suite_name": "orders_data_product_quality",
  "data_asset_type": "Dataset",
  "meta": {
    "great_expectations_version": "0.18.0",
    "data_product": "orders-dp",
    "owner": "order-team",
    "sla": {
      "freshness_minutes": 5,
      "completeness_percent": 99.9
    }
  },
  "expectations": [
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 0
      },
      "meta": {
        "notes": "Table should not be empty in normal operations"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "order_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "customer_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "status"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "priority"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "order_id"
      },
      "meta": {
        "notes": "Primary key - must never be null"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "customer_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "status"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_unique",
      "kwargs": {
        "column": "order_id"
      },
      "meta": {
        "notes": "Order ID must be unique across all records"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "status",
        "value_set": [
          "received",
          "validated",
          "wave_assigned",
          "picking",
          "consolidating",
          "packing",
          "shipping",
          "shipped",
          "completed",
          "cancelled"
        ]
      },
      "meta": {
        "notes": "Valid order status values from domain model"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "priority",
        "value_set": [
          "same_day",
          "next_day",
          "standard"
        ]
      },
      "meta": {
        "notes": "Valid priority values from domain model"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_of_type",
      "kwargs": {
        "column": "created_at",
        "type_": "TIMESTAMP"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "created_at",
        "min_value": "2024-01-01T00:00:00",
        "parse_strings_as_datetimes": true
      },
      "meta": {
        "notes": "Orders should not have dates before system launch"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "order_id",
        "regex": "^ORD-[A-Z0-9]{8,}$"
      },
      "meta": {
        "notes": "Order ID format: ORD-XXXXXXXX"
      }
    },
    {
      "expectation_type": "expect_column_pair_values_A_to_be_greater_than_B",
      "kwargs": {
        "column_A": "updated_at",
        "column_B": "created_at",
        "or_equal": true
      },
      "meta": {
        "notes": "Updated timestamp must be >= created timestamp"
      }
    }
  ],
  "ge_cloud_id": null,
  "data_context_key": "orders_data_product"
}
