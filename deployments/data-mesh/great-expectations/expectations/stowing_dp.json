{
  "expectation_suite_name": "stowing_data_product_quality",
  "data_asset_type": "Dataset",
  "meta": {
    "great_expectations_version": "0.18.0",
    "data_product": "stowing-dp",
    "owner": "receiving-team",
    "sla": {
      "freshness_minutes": 5,
      "completeness_percent": 99.9
    }
  },
  "expectations": [
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 0
      },
      "meta": {
        "notes": "Table should not be empty in normal operations"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "stow_task_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "receipt_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "worker_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "status"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "sku"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "target_location"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "stow_task_id"
      },
      "meta": {
        "notes": "Primary key - must never be null"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "sku"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "status"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_unique",
      "kwargs": {
        "column": "stow_task_id"
      },
      "meta": {
        "notes": "Stow task ID must be unique across all records"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "status",
        "value_set": [
          "pending",
          "assigned",
          "in_progress",
          "completed",
          "cancelled",
          "exception"
        ]
      },
      "meta": {
        "notes": "Valid stow task status values from domain model"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "stow_task_id",
        "regex": "^STW-[A-Z0-9]{8,}$"
      },
      "meta": {
        "notes": "Stow task ID format: STW-XXXXXXXX"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "sku",
        "regex": "^[A-Z0-9]{3,20}$"
      },
      "meta": {
        "notes": "SKU format: 3-20 alphanumeric characters"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "target_location",
        "regex": "^[A-Z]-\\d{2}-\\d{2}-\\d{2}$"
      },
      "meta": {
        "notes": "Location format: Zone-Aisle-Rack-Level (e.g., A-01-02-03)"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "quantity",
        "min_value": 1
      },
      "meta": {
        "notes": "Quantity must be at least 1"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_of_type",
      "kwargs": {
        "column": "created_at",
        "type_": "TIMESTAMP"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "created_at",
        "min_value": "2024-01-01T00:00:00",
        "parse_strings_as_datetimes": true
      },
      "meta": {
        "notes": "Stow tasks should not have dates before system launch"
      }
    },
    {
      "expectation_type": "expect_column_pair_values_A_to_be_greater_than_B",
      "kwargs": {
        "column_A": "completed_at",
        "column_B": "started_at",
        "or_equal": true
      },
      "meta": {
        "notes": "Completed timestamp must be >= started timestamp"
      }
    }
  ],
  "ge_cloud_id": null,
  "data_context_key": "stowing_data_product"
}
