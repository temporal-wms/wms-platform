{
  "expectation_suite_name": "picking_data_product_quality",
  "data_asset_type": "Dataset",
  "meta": {
    "great_expectations_version": "0.18.0",
    "data_product": "picking-dp",
    "owner": "operations-team",
    "sla": {
      "freshness_minutes": 5,
      "completeness_percent": 99.9
    }
  },
  "expectations": [
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 0
      },
      "meta": {
        "notes": "Table should not be empty in normal operations"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "task_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "order_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "wave_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "picker_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "status"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "method"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "task_id"
      },
      "meta": {
        "notes": "Primary key - must never be null"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "order_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "status"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_unique",
      "kwargs": {
        "column": "task_id"
      },
      "meta": {
        "notes": "Task ID must be unique across all records"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "status",
        "value_set": [
          "pending",
          "assigned",
          "in_progress",
          "completed",
          "cancelled",
          "exception"
        ]
      },
      "meta": {
        "notes": "Valid pick task status values from domain model"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "method",
        "value_set": [
          "single",
          "batch",
          "zone",
          "wave"
        ]
      },
      "meta": {
        "notes": "Valid pick method values from domain model"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_of_type",
      "kwargs": {
        "column": "created_at",
        "type_": "TIMESTAMP"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "created_at",
        "min_value": "2024-01-01T00:00:00",
        "parse_strings_as_datetimes": true
      },
      "meta": {
        "notes": "Pick tasks should not have dates before system launch"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "task_id",
        "regex": "^PICK-[A-Z0-9-]+$"
      },
      "meta": {
        "notes": "Pick task ID format: PICK-XXXXXXXX"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "total_items",
        "min_value": 1
      },
      "meta": {
        "notes": "Pick task must have at least one item"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "picked_items",
        "min_value": 0
      },
      "meta": {
        "notes": "Picked items count must be non-negative"
      }
    },
    {
      "expectation_type": "expect_column_pair_values_A_to_be_greater_than_B",
      "kwargs": {
        "column_A": "total_items",
        "column_B": "picked_items",
        "or_equal": true
      },
      "meta": {
        "notes": "Picked items cannot exceed total items"
      }
    },
    {
      "expectation_type": "expect_column_pair_values_A_to_be_greater_than_B",
      "kwargs": {
        "column_A": "updated_at",
        "column_B": "created_at",
        "or_equal": true
      },
      "meta": {
        "notes": "Updated timestamp must be >= created timestamp"
      }
    }
  ],
  "ge_cloud_id": null,
  "data_context_key": "picking_data_product"
}
