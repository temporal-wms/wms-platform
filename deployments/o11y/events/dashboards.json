          "textMode": "auto"
        },
        "queries": [
          {
            "refId": "A",
            "expr": "sum(rate(wms_outbox_events_published_total{service=\"${SERVICE}\", status=\"error\"}[$__rate_interval])) / sum(rate(wms_outbox_events_published_total{service=\"${SERVICE}\"}[$__rate_interval])) or vector(0)"
