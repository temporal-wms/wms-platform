      {
        "id": 25,
        "title": "Outbox Failed Events (by Event Type)",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 53, "w": 12, "h": 8},
        "description": "Rate of failed outbox events by event type",
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "palette-classic"},
            "custom": {
