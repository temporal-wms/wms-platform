      {
        "id": 24,
        "title": "Outbox Published Events (by Event Type)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 53, "w": 12, "h": 8},
        "description": "Rate of events published from outbox by event type",
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
