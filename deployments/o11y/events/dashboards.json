            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "queries": [
          {
            "refId": "A",
            "legendFormat": "{{event_type}}",
            "expr": "sum(rate(wms_kafka_events_consumed_total{service=\"${SERVICE}\"}[$__rate_interval])) by (event_type)"
          }
        ]
      },
      {
        "id": 14,
        "title": "Consumer Lag",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 31, "w": 12, "h": 8},
        "description": "Kafka consumer lag (messages behind)",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "never",
              "thresholdsStyle": {"mode": "area"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 1000}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "queries": [
          {
            "refId": "A",
            "legendFormat": "{{topic}} - partition {{partition}}",
            "expr": "sum(wms_kafka_consumer_lag{service=\"${SERVICE}\"}) by (topic, partition)"
          }
        ]
      },
      {
        "id": 15,
        "title": "Events Consumed by Status",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 31, "w": 12, "h": 8},
        "description": "Event consume rate by status (success/error)",
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "never"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*error.*"},
              "properties": [{"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*success.*"},
              "properties": [{"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}}]
            }
          ]
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "sum"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "queries": [
          {
            "refId": "A",
            "legendFormat": "{{status}}",
            "expr": "sum(rate(wms_kafka_events_consumed_total{service=\"${SERVICE}\"}[$__rate_interval])) by (status)"
          }
        ]
      },
      {
        "id": 16,
        "title": "Event Types Breakdown",
        "type": "row",
        "gridPos": {"x": 0, "y": 39, "w": 24, "h": 1},
        "collapsed": false
      },
      {
        "id": 17,
        "title": "Published Events by Type",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 40, "w": 12, "h": 8},
        "description": "Distribution of published events by type",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]},
          "tooltip": {"mode": "single"},
          "pieType": "pie",
          "reduceOptions": {"calcs": ["sum"], "fields": "", "values": false}
        },
        "queries": [
          {
            "refId": "A",
            "legendFormat": "{{event_type}}",
            "expr": "sum(increase(wms_kafka_events_published_total{service=\"${SERVICE}\"}[$__range])) by (event_type)"
          }
        ]
      },
      {
        "id": 18,
        "title": "Consumed Events by Type",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 40, "w": 12, "h": 8},
        "description": "Distribution of consumed events by type",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]},
          "tooltip": {"mode": "single"},
          "pieType": "pie",
          "reduceOptions": {"calcs": ["sum"], "fields": "", "values": false}
        },
        "queries": [
          {
            "refId": "A",
