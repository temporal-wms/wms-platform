      {
        "id": 19,
        "title": "Outbox",
        "type": "row",
        "gridPos": {"x": 0, "y": 48, "w": 24, "h": 1},
        "collapsed": false
      },
      {
        "id": 20,
        "title": "Outbox Events Pending",
        "type": "stat",
        "gridPos": {"x": 0, "y": 49, "w": 6, "h": 4},
        "description": "Number of events waiting to be published from outbox",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "orange", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
          "textMode": "auto"
        },
        "queries": [
          {
            "refId": "A",
            "expr": "wms_outbox_events_pending{service=\"${SERVICE}\"} or vector(0)"
          }
        ]
      },
      {
        "id": 21,
        "title": "Outbox Publish Rate",
        "type": "stat",
        "gridPos": {"x": 6, "y": 49, "w": 6, "h": 4},
        "description": "Rate of events published from outbox",
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [{"color": "green", "value": null}]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
          "textMode": "auto"
        },
        "queries": [
          {
            "refId": "A",
            "expr": "sum(rate(wms_outbox_events_published_total{service=\"${SERVICE}\", status=\"success\"}[$__rate_interval])) or vector(0)"
          }
        ]
      },
      {
        "id": 22,
        "title": "Outbox Error Rate %",
        "type": "stat",
        "gridPos": {"x": 12, "y": 49, "w": 6, "h": 4},
        "description": "Percentage of outbox publish failures",
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "orange", "value": 0.05},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
